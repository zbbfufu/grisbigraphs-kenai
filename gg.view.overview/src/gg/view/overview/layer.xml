<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="gg-view-overview-OverviewAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="gg.view.overview.Bundle"/>
                <attr name="component" methodvalue="gg.view.overview.OverviewTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="gg.view.overview.Bundle#CTL_OverviewAction"/>
                <attr name="iconBase" stringvalue="gg/resources/icons/Overview.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
            <file name="gg-view-overview-IncomeExpensesAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="gg.view.overview.Bundle"/>
                <attr name="component" methodvalue="gg.view.overview.IncomeExpensesTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="gg.view.overview.Bundle#CTL_IncomeExpensesAction"/>
                <attr name="iconBase" stringvalue="gg/resources/icons/IncomeExpenses.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="View">
            <file name="OverviewAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-overview-OverviewAction.instance"/>
                <attr name="position" intvalue="10"/>
            </file>
        </folder>
        <folder name="Window">
            <file name="OverviewAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-overview-OverviewAction.instance"/>
                <attr name="position" intvalue="10"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="View">
            <file name="OverviewAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-overview-OverviewAction.instance"/>
                <attr name="position" intvalue="10"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="IncomeExpensesTopComponent.settings" url="IncomeExpensesTopComponentSettings.xml"/>
            <file name="OverviewTopComponent.settings" url="OverviewTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="OverviewTopComponent.wstcref" url="OverviewTopComponentWstcref.xml">
                    <attr name="position" intvalue="10"/>
                </file>
            </folder>
            <folder name="output">
                <file name="IncomeExpensesTopComponent.wstcref" url="IncomeExpensesTopComponentWstcref.xml">
                    <attr name="position" intvalue="10"/>
                </file>
            </folder>
        </folder>
        <folder name="Groups">
            <file name="OverviewGroup.wsgrp" url="groups/OverviewGroupWsgrp.xml"/>
            <folder name="OverviewGroup">
                <file name="IncomeExpensesTopComponent.wstcgrp" url="groups/OverviewGroup/IncomeExpensesTopComponentWstcgrp.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
