<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="gg-view-categoriesbalances-CategoriesBalancesAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="gg.view.categoriesbalances.Bundle"/>
                <attr name="component" methodvalue="gg.view.categoriesbalances.CategoriesBalancesTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="gg.view.categoriesbalances.Bundle#CTL_CategoriesBalancesAction"/>
                <attr name="iconBase" stringvalue="gg/resources/icons/CategoriesBalances.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
            <file name="gg-view-categoriesbalances-GraphCategoriesBalancesAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="gg.view.categoriesbalances.Bundle"/>
                <attr name="component" methodvalue="gg.view.categoriesbalances.GraphCategoriesBalancesTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="gg.view.categoriesbalances.Bundle#CTL_GraphCategoriesBalancesAction"/>
                <attr name="iconBase" stringvalue="gg/resources/icons/GraphCategoriesBalances.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="View">
            <file name="CategoriesBalancesAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-categoriesbalances-CategoriesBalancesAction.instance"/>
                <attr name="position" intvalue="40"/>
            </file>
        </folder>
        <folder name="Window">
            <file name="CategoriesBalancesAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-categoriesbalances-CategoriesBalancesAction.instance"/>
                <attr name="position" intvalue="40"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="View">
            <file name="CategoriesBalancesAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-categoriesbalances-CategoriesBalancesAction.instance"/>
                <attr name="position" intvalue="40"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="CategoriesBalancesTopComponent.settings" url="CategoriesBalancesTopComponentSettings.xml"/>
            <file name="GraphCategoriesBalancesTopComponent.settings" url="GraphCategoriesBalancesTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="CategoriesBalancesTopComponent.wstcref" url="CategoriesBalancesTopComponentWstcref.xml">
                    <attr name="position" intvalue="40"/>
                </file>
            </folder>
            <folder name="output">
                <file name="GraphCategoriesBalancesTopComponent.wstcref" url="GraphCategoriesBalancesTopComponentWstcref.xml">
                    <attr name="position" intvalue="40"/>
                </file>
            </folder>
        </folder>
        <folder name="Groups">
            <file name="CategoriesBalancesGroup.wsgrp" url="groups/CategoriesBalancesGroupWsgrp.xml"/>
            <folder name="CategoriesBalancesGroup">
                <file name="SearchFilterTopComponent.wstcgrp" url="groups/CategoriesBalancesGroup/SearchFilterTopComponentWstcgrp.xml"/>
                <file name="GraphCategoriesBalancesTopComponent.wstcgrp" url="groups/CategoriesBalancesGroup/GraphCategoriesBalancesTopComponentWstcgrp.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
