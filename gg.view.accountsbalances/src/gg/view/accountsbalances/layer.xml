<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="gg-view-accountsbalances-AccountsBalancesAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="gg.view.accountsbalances.Bundle"/>
                <attr name="component" methodvalue="gg.view.accountsbalances.AccountsBalancesTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="gg.view.accountsbalances.Bundle#CTL_AccountsBalancesAction"/>
                <attr name="iconBase" stringvalue="gg/resources/icons/AccountsBalances.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
            <file name="gg-view-accountsbalances-GraphAccountsBalancesAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="gg.view.accountsbalances.Bundle"/>
                <attr name="component" methodvalue="gg.view.accountsbalances.GraphAccountsBalancesTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="gg.view.accountsbalances.Bundle#CTL_GraphAccountsBalancesAction"/>
                <attr name="iconBase" stringvalue="gg/resources/icons/GraphAccountsBalances.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="View">
            <file name="AccountsBalancesAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-accountsbalances-AccountsBalancesAction.instance"/>
                <attr name="position" intvalue="20"/>
            </file>
        </folder>
        <folder name="Window">
            <file name="AccountsBalancesAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-accountsbalances-AccountsBalancesAction.instance"/>
                <attr name="position" intvalue="20"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="View">
            <file name="AccountsBalancesAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-accountsbalances-AccountsBalancesAction.instance"/>
                <attr name="position" intvalue="20"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="AccountsBalancesTopComponent.settings" url="AccountsBalancesTopComponentSettings.xml"/>
            <file name="GraphAccountsBalancesTopComponent.settings" url="GraphAccountsBalancesTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="AccountsBalancesTopComponent.wstcref" url="AccountsBalancesTopComponentWstcref.xml">
                    <attr name="position" intvalue="20"/>
                </file>
            </folder>
            <folder name="output">
                <file name="GraphAccountsBalancesTopComponent.wstcref" url="GraphAccountsBalancesTopComponentWstcref.xml">
                    <attr name="position" intvalue="20"/>
                </file>
            </folder>
        </folder>
        <folder name="Groups">
            <file name="AccountsBalancesGroup.wsgrp" url="groups/AccountsBalancesGroupWsgrp.xml"/>
            <folder name="AccountsBalancesGroup">
                <file name="SearchFilterTopComponent.wstcgrp" url="groups/AccountsBalancesGroup/SearchFilterTopComponentWstcgrp.xml"/>
                <file name="GraphAccountsBalancesTopComponent.wstcgrp" url="groups/AccountsBalancesGroup/GraphAccountsBalancesTopComponentWstcgrp.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
