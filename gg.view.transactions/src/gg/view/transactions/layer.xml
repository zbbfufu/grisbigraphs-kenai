<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="gg-view-transactions-TransactionsAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="gg.view.transactions.Bundle"/>
                <attr name="component" methodvalue="gg.view.transactions.TransactionsTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="gg.view.transactions.Bundle#CTL_TransactionsAction"/>
                <attr name="iconBase" stringvalue="gg/resources/icons/Transactions.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="View">
            <file name="TransactionsAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-transactions-TransactionsAction.instance"/>
                <attr name="position" intvalue="50"/>
            </file>
        </folder>
        <folder name="Window">
            <file name="TransactionsAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-transactions-TransactionsAction.instance"/>
                <attr name="position" intvalue="50"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="View">
            <file name="TransactionsAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/gg-view-transactions-TransactionsAction.instance"/>
                <attr name="position" intvalue="50"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="TransactionsTopComponent.settings" url="TransactionsTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="TransactionsTopComponent.wstcref" url="TransactionsTopComponentWstcref.xml">
                    <attr name="position" intvalue="50"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
