<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="OptionsDialog">
        <file name="Advanced.instance_hidden"/>
        <file name="Advanced_hidden"/>
        <file name="General.instance_hidden"/>
        <file name="Keymaps.instance_hidden"/>
    </folder>
    <folder name="OptionsDialog">
        <file name="GeneralOptionsCategory.instance">
            <attr name="categoryName" bundlevalue="gg.options.Bundle#OptionsCategory_Name_General"/>
            <attr name="controller" newvalue="gg.options.GeneralOptionsPanelController"/>
            <attr name="iconBase" stringvalue="gg/resources/icons/GeneralOptions.png"/>
            <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.OptionsCategory.createCategory"/>
            <attr name="keywords" bundlevalue="gg.options.Bundle#OptionsCategory_Keywords_General"/>
            <attr name="keywordsCategory" stringvalue="General"/>
            <attr name="title" bundlevalue="gg.options.Bundle#OptionsCategory_Title_General"/>
        </file>
    </folder>
</filesystem>
