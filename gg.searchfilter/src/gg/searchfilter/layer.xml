<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="gg-searchfilter-SearchFilterAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="gg.searchfilter.Bundle"/>
                <attr name="component" methodvalue="gg.searchfilter.SearchFilterTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="gg.searchfilter.Bundle#CTL_SearchFilterAction"/>
                <attr name="iconBase" stringvalue="gg/resources/icons/SearchFilter.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="SearchFilterTopComponent.settings" url="SearchFilterTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="explorer">
                <file name="SearchFilterTopComponent.wstcref" url="SearchFilterTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
